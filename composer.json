{
    "name": "mjolnic/picnic",
    "description": "Laravel 4 On Demand Image Post-processing package (Image manipulation on the fly).",
    "keywords": ["thumbnail", "thumb", "wideimage", "assets", "laravel", "thorcms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Aguilar",
            "homepage": "http://www.mjolnic.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "illuminate/support": "4.2.*",
        "smottt/wideimage": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Mjolnic\\Picnic\\": "src"
        }
    },
    "config": {
        "vendor-dir": "vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
